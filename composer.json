{
	"name": "rivervanrain/google_api",
	"type": "elgg-plugin",
        "description": "Google API, Client and Services for Elgg",
        "license": "AGPL-3.0-or-later",
	"keywords": [
		"development",
		"google"
	],
	"homepage": "https://wzm.me",
	"authors": [
		{
		  "name": "Nikolai Shcherbin",
		  "email": "support@wzm.me"
		}
	],
	"config": {
        "fxp-asset": {
            "enabled": false
        },
		"allow-plugins": {
			"composer/installers": true
		}
        },
	"require": {
		"php": "^8.1",
		"composer/installers": "^1.0.8",
		"google/apiclient": "^2.15.0"
	},
	"autoload": {
		"psr-0": {
			"": "classes/"
		}
	},
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
	"conflict": {
		"elgg/elgg": "<5.0"
	},
	"support": {
		"source": "https://github.com/rivervanrain/google_api"
	}
}
